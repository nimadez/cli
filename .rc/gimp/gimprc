# /tmp/
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(swap-compression "none")
(tile-cache-size 16G)
(prev-language "American English")
(config-version "3.0.4")
(undo-size 1024M)
(undo-preview-size medium)
(save-document-history no)
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(debug-policy never)
(check-updates no)
(check-update-timestamp 1757438735)
(snap-distance 10)
(cursor-mode crosshair)
(image-title-format "%f-%p.%i (%t) %wx%h")
(monitor-xresolution 94)
(monitor-yresolution 94)
(default-view
